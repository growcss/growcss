{
  "name": "growcss",
  "private": true,
  "license": "MIT",
  "workspaces": [
    "packages/coding-standard/*",
    "packages/core/*"
  ],
  "scripts": {
    "bootstrap": "                    node verify-node-version.js && lerna bootstrap",
    "build": "                        node verify-node-version.js && yarn run sort-package && cross-env NODE_ENV=production lerna run build",
    "changed": "                      node verify-node-version.js && lerna changed",
    "changelog": "                    node verify-node-version.js && lerna-changelog",
    "clear": "                        yarn run clear:modules && yarn run clear:build",
    "clear:build": "                  node verify-node-version.js && lerna exec -- rimraf dist",
    "clear:modules": "                node verify-node-version.js && lerna clean --yes",
    "lint": "                         yarn run lint:eslint:source && yarn run lint:eslint:supporting && yarn run lint:style",
    "lint:eslint:source": "           node verify-node-version.js && eslint -c .eslintrc.sources.js --print-config .eslintrc.sources.js | eslint-config-prettier-check && eslint -c .eslintrc.sources.js 'packages/*/*/src/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint:source:staged": "    yarn run lint:eslint:source --fix --max-warnings=1",
    "lint:eslint:supporting": "       node verify-node-version.js && eslint -c .eslintrc.supporting.js --print-config .eslintrc.supporting.js | eslint-config-prettier-check && eslint -c .eslintrc.supporting.js 'packages/*/*/{__tests__,__stories__}/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint:supporting:staged": "yarn run lint:eslint:supporting --fix --max-warnings=1",
    "lint:fix": "                     yarn run lint:fix:eslint:source && yarn run lint:fix:eslint:supporting",
    "lint:fix:eslint:source": "       yarn run lint:eslint:source --fix",
    "lint:fix:eslint:supporting": "   yarn run lint:eslint:supporting --fix",
    "lint:style": "                   node verify-node-version.js && stylelint-config-prettier-check && stylelint 'packages/*/*/src/**/*.+{js,ts,tsx}'",
    "sort-package": "                 lerna exec -- sort-package-json",
    "storybook": "                    node verify-node-version.js && cross-env NODE_ENV=storybook start-storybook -p 6006 -c .storybook",
    "storybook-build": "              cross-env NODE_ENV=production build-storyboo k-c .storybook",
    "test": "                         node verify-node-version.js && cross-env NODE_ENV=test jest",
    "test:changed": "                 cross-env NODE_ENV=test CHANGED_PACKAGES=$(yarn run changed) COVERAGE_PACKAGES=$(yarn run changed) jest",
    "test:coverage": "                yarn run test --coverage",
    "test:watch": "                   yarn run test --watch"
  },
