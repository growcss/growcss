    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "types-installer": "^1.4.0",
    "typescript": "^3.1.6",
    "typescript-plugin-styled-components": "^1.0.0"
  },
  "engines": {
    "node": "^9.10.0",
    "npm": "^6.0.0",
    "yarn": "^1.3.0"
  },
  "lint-staged": {
    "packages/behaviors/**/src/**/*.{js, jsx}": [
      "prettier --write",
      "git add"
    ],
    "packages/components/**/src/**/*.{js, jsx}": [
      "prettier --write",
      "git add"
    ],
    "packages/utils/**/src/**/*.{js, jsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "workspaces": [
    "packages/behaviors/*",
    "packages/components/*",
    "packages/utils/*"
  ],
  "jest-junit": {
    "output": "test-reports/junit.xml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
