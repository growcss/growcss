{
  "name": "growcss",
  "private": true,
  "license": "MIT",
  "scripts": {
    "bootstrap": "                 yarn run delete && lerna bootstrap",
    "build": "                     yarn run link && lerna exec -- sort-package-json && cross-env NODE_ENV=production lerna run build --parallel",
    "changed": "                   lerna changed",
    "changelog": "                 lerna-changelog",
    "delete": "                    yarn run delete:modules && yarn run delete:build",
    "delete:build": "              lerna exec -- rimraf dist",
    "delete:modules": "            lerna clean --yes",
    "docz:dev": "                  docz dev",
    "docz:build": "                docz build",
    "link": "                      lerna link",
    "lint": "                      yarn run lint:eslint && yarn run lint:tslint && yarn run lint:stylelint",
    "lint:stylelint": "            stylelint '**/*.{js,ts,tsx}'",
    "lint:eslint": "               eslint --print-config .eslintrc | eslint-config-prettier-check && eslint '**/*.js'",
    "lint:fix": "                  yarn run lint:fix:eslint && yarn run lint:fix:tslint:sources && yarn run lint:fix:tslint:supporting && yarn run lint:fix:stylelint",
    "lint:fix:eslint": "           eslint --fix '**/*.js'",
    "lint:fix:tslint:sources": "   tslint --fix -c ./tslint.sources.json --format stylish 'packages/*/*/src/**/*.+(ts|tsx)' --exclude '**/__tests__/**'",
    "lint:fix:tslint:supporting": "tslint --fix -c ./tslint.supporting.json --format stylish 'packages/*/*/+(tests|__tests__|examples|docs|example-helpers)/**/*.(ts|tsx)' 'packages/*/*/src/**/__tests__/**/*.+(ts|tsx)'",
    "lint:fix:stylelint": "        stylelint '**/*.{js,ts,tsx}' --fix",
    "lint:tslint": "               yarn run lint:tslint:sources && yarn run lint:tslint:supporting",
    "lint:tslint:sources": "       tslint-config-prettier-check ./tslint.sources.json && tslint -c ./tslint.sources.json --format stylish 'packages/*/*/src/**/*.+(ts|tsx)' --exclude '**/__tests__/**'",
    "lint:tslint:supporting": "    tslint-config-prettier-check ./tslint.supporting.json && tslint -c ./tslint.supporting.json --format stylish 'packages/*/*/+(tests|__tests__|examples|docs|example-helpers)/**/*.(ts|tsx)' 'packages/*/*/src/**/__tests__/**/*.+(ts|tsx)'",
    "storybook": "                 cross-env NODE_ENV=production start-storybook -p 6006",
    "storybook-build": "           cross-env NODE_ENV=production build-storybook",
    "typecheck": "                 yarn run typecheck:typescript",
    "typecheck:typescript": "      tsc --noEmit --project ./tsconfig.typecheck.json",
    "test": "                      cross-env NODE_ENV=test jest",
    "test:changed": "              cross-env NODE_ENV=test CHANGED_PACKAGES=$(yarn run changed) COVERAGE_PACKAGES=$(yarn run changed) jest",
    "test:ci": "                   yarn run test --coverage -- --runInBand",
    "test:coverage": "             yarn run test --coverage",
    "test:watch": "                yarn run test --watch"
  },
  "dependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-typescript": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
