{
  "name": "growcss",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/PrisisForks/growcss.git"
  },
  "license": "MIT",
  "scripts": {
    "audit": "                        lerna exec npm audit",
    "bootstrap": "                    node verify-node-version.js && lerna bootstrap",
    "build": "                        node verify-node-version.js && npm run clear:build && git rev-parse HEAD > .commithash && cross-env NODE_ENV=production lerna run build",
    "clear": "                        npm run clear:modules && npm run clear:build",
    "clear:build": "                  node verify-node-version.js && lerna exec -- rimraf dist",
    "clear:modules": "                node verify-node-version.js && lerna clean --yes",
    "commit": "                       git-cz",
    "hooks:install": "                node verify-node-version.js && node node_modules/husky/husky.js install",
    "hooks:uninstall": "              node verify-node-version.js && node node_modules/husky/husky.js uninstall",
    "preinstall": "                   node verify-node-version.js",
    "lint": "                         npm run lint:eslint:source && npm run lint:eslint:supporting && npm run lint:style",
    "lint:eslint:source": "           node verify-node-version.js && eslint -c .eslintrc.sources.js --print-config .eslintrc.sources.js | eslint-config-prettier-check && eslint -c .eslintrc.sources.js 'packages/**/**/src/**/*.{js,jsx,ts,tsx}'",
    "lint:eslint:supporting": "       node verify-node-version.js && eslint -c .eslintrc.supporting.js --print-config .eslintrc.supporting.js | eslint-config-prettier-check && eslint -c .eslintrc.supporting.js 'packages/**/**/{__tests__,__stories__}/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "                     npm run lint:fix:eslint:source && npm run lint:fix:eslint:supporting",
    "lint:fix:eslint:source": "       npm run lint:eslint:source -- --fix",
    "lint:fix:eslint:supporting": "   npm run lint:eslint:supporting -- --fix",
    "lint:style": "                   node verify-node-version.js && stylelint-config-prettier-check && stylelint 'packages/**/**/src/**/*.+{js,ts,tsx}'",
    "sort-package": "                 lerna exec -- sort-package-json",
    "storybook": "                    node verify-node-version.js && lerna bootstrap && npm run build && cross-env NODE_ENV=development start-storybook -p 6006 -c .storybook",
    "storybook:build": "              cross-env NODE_ENV=development build-storybook -c .storybook",
    "storybook:screenshot": "         cross-env NODE_ENV=development storybook-chrome-screenshot -p 9001 -c .storybook",
    "test": "                         node verify-node-version.js && cross-env NODE_ENV=test jest",
    "test:changed": "                 node verify-node-version.js && cross-env NODE_ENV=test CHANGED_PACKAGES=$(node build/ci-scripts/get-changed-packages-since-master.js) COVERAGE_PACKAGES=$(node build/ci-scripts/get-code-threshold-for-changed-packages-since-master.js) jest",
    "test:coverage": "                node verify-node-version.js && cross-env NODE_ENV=test CHANGED_PACKAGES=$(node build/ci-scripts/get-all-packages.js) COVERAGE_PACKAGES=$(node build/ci-scripts/get-code-threshold-for-changed-packages-since-master.js) jest",
    "test:watch": "                   npm run test -- --watch",
    "upgrade-interactive": "          node verify-node-version.js && npm-check --update"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged || node build/on-lint-error.js && npm run sort-package",
      "postcommit": "git update-index --again"
    }
  },
  "lint-staged": {
    "ignore": [
      "**/dist/**",
      "**/node_modules/**"
    ],
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "npm run lint:eslint:source -- --fix --max-warnings=0",
        "npm run lint:eslint:supporting -- --fix --max-warnings=0",
        "git add"
      ],
      "*.{md}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
