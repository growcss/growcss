    "jest-styled-components": "^6.3.0",
    "jsdom": "^13.0.0",
    "jsdom-global": "^3.0.2",
    "lerna": "^3.4.3",
    "lint-staged": "^8.0.4",
    "preact": "^8.2.7",
    "preact-compat": "^3.18.0",
    "prettier": "^1.10.2",
    "prettier-eslint": "^8.8.1",
