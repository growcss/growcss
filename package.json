    "npm": "^6.1.0"
  },
  "jest-junit": {
    "output": "test-reports/junit.xml"
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx}": [
        "npm run lint:eslint:source -- --fix --max-warnings=0",
        "npm run lint:eslint:supporting -- --fix --max-warnings=0",
        "git add"
      ],
      "*.{md}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/**",
      "**/node_modules/**"
    ]
  }
